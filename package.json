{
  "name": "vscode-extension-vue",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "main": "./dist/js/client.js",
  "activationEvents": [
    "onLanguage:vue"
  ],
  "contributes": {
    "languages": [
      {
        "id": "vue",
        "configuration": "./syntax/vue-language-configuration.json",
        "aliases": [
          "Vue",
          "vue"
        ],
        "extensions": [
          ".vue"
        ]
      }
    ],
    "grammars": [
      {
        "language": "vue",
        "scopeName": "source.vue",
        "path": "./syntaxes/vue.tmLanguage.json",
        "embeddedLanguages": {
          "text.html": "html",
          "text.markdown": "md",
          "source.css": "css",
          "source.scss": "scss",
          "source.less": "less",
          "source.sass": "sass",
          "source.stylus": "stylus",
          "source.js": "javascript",
          "source.ts": "typescript",
          "source.yaml": "yaml",
          "source.json": "json"
        }
      }
    ]
  },
  "dependencies": {
    "line-column": "^1.0.2",
    "lodash": "^4.17.20",
    "path-browserify": "^1.0.1",
    "tslib": "^2.0.1",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@types/copy-webpack-plugin": "^6.0.0",
    "@types/friendly-errors-webpack-plugin": "^0.1.2",
    "@types/generate-json-webpack-plugin": "^0.3.1",
    "@types/line-column": "^1.0.0",
    "@types/lodash": "^4.14.159",
    "@types/node": "^14.0.27",
    "@types/terser-webpack-plugin": "^4.1.0",
    "@types/webpack-bundle-analyzer": "^3.8.0",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "chalk": "^4.1.0",
    "copy-webpack-plugin": "^6.0.3",
    "eslint": "^7.7.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "generate-json-webpack-plugin": "^1.0.0",
    "progress-bar-webpack-plugin": "^2.1.0",
    "terser-webpack-plugin": "^4.1.0",
    "ts-loader": "^8.0.2",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0"
  }
}
