{
  "name": "xvue",
  "version": "1.0.0",
  "description": "vue utils",
  "author": "xiaoboost",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaoboost/vscode-extension-vue"
  },
  "ava": {
    "serial": true,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register/transpile-only.js",
      "tsconfig-paths/register.js"
    ],
    "environmentVariables": {
      "TS_NODE_PROJECT": "tsconfig.test.json"
    },
    "files": [
      "tests/**/*.spec.ts"
    ]
  },
  "devDependencies": {
    "ava": "^3.13.0",
    "nyc": "^15.1.0"
  }
}
