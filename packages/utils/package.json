{
  "name": "@xvue/utils",
  "version": "1.0.0",
  "description": "utils for xvue",
  "author": "xiaoboost",
  "license": "ISC",
  "module": ".",
  "types": ".",
  "scripts": {
    "clear": "rimraf ./dist",
    "dev": "tsc -w -p ./tsconfig.dev.json",
    "build:es": "tsc -p ./tsconfig.build.json",
    "build": "pnpm run clear && pnpm run build:es",
    "prepare": "pnpm run build"
  },
  "ava": {
    "serial": true,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register/transpile-only.js",
      "tsconfig-paths/register.js"
    ],
    "environmentVariables": {
      "TS_NODE_PROJECT": "tsconfig.test.json"
    },
    "files": [
      "tests/**/*.spec.ts"
    ]
  },
  "dependencies": {
    "tslib": "^2.0.1"
  },
  "devDependencies": {
    "ava": "^3.12.1",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "ts-node": "~8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3"
  }
}
